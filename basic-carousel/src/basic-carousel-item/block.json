{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "jonny/basic-carousel-item",
	"version": "0.1.0",
	"title": "Basic Carousel Item",
	"category": "widgets",
	"icon": "smiley",
	"description": "Item for  basic caousel, it feches placeholder images rom fetch api",
	"example": {},
	"supports": {
		"html": false
	},
	"parent": ["jonny/basic-carousel"],
	"attributes": {
		"name":{
			"type": "string",
			"default": "Item name"
		},
		"quote":{
			"type": "string",
			"default": "Quote"
		},
		"avatar": {
			"type": "object",
			"default": {
				"url": "",
				"alt": "Avatar",
				"title": "Avatar"
			}
		}
	},
	"textdomain": "basic-carousel-item",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}